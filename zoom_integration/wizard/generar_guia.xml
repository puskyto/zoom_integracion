    <record id="sale_generar_guia_envio_view_form" model="ir.ui.view">
        <field name="name">sale.generar.guia.envio.form</field>
        <field name="model">stock.picking.zoom</field>
        <field name="arch" type="xml">
            <form string="Generar Guia de Movilizacion">
                <group>
                    <group>
                        <field name="metodo"/>
                    </group>
                </group>
                <footer>
                    <button name="generar_guia" string="Generar" type="object" class="oe_highlight"/>
